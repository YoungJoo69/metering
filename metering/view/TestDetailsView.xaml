                TabIndex="3"  
                Margin="5"
                Width="180"                     
                HorizontalContentAlignment="Center">
                <TextBox.Text>
                    <Binding Source="{StaticResource measurementInterval}" 
                                         Path="MeasurementInterval"
                                         UpdateSourceTrigger="PropertyChanged"/>
                </TextBox.Text>

            </TextBox>

            <!-- Start Measurement Delay -->
            <TextBox 
                x:Name="startMeasurementDelay"
                Grid.Column="3"
                materialDesign:HintAssist.Hint="{x:Static resources:Strings.header_start_measurement_delay}"
                Style="{StaticResource MaterialDesignFloatingHintTextBox}" 
                TabIndex="4"  
                Margin="5"
                Width="180"                     
                HorizontalContentAlignment="Center">
                <TextBox.Text>
                    <Binding Source="{StaticResource startMeasurementDelay}" 
                                         Path="StartMeasurementDelay"
                                         UpdateSourceTrigger="PropertyChanged"/>
                </TextBox.Text>

            </TextBox>

        </Grid>

