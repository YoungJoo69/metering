<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:metering.view"
                    xmlns:viewModel="clr-namespace:metering.viewModel">

    <DataTemplate DataType="{x:Type viewModel:CommandsViewModel}">
        <view:CommandsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:CommunicationViewModel}">
        <view:CommunicationView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:NominalValuesViewModel}">
        <view:NominalValuesView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:TestDetailsViewModel}">
        <view:TestDetailsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:TestDetailViewModel}">
        <view:TestDetailView/>
    </DataTemplate>
</ResourceDictionary>